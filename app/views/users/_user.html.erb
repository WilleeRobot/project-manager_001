<tr>
  <td><%= user.username %></td>
  <td><%= link_to "Show", user_path(user) %></td>
  <% if !user.email.blank? %>
    <td><a href="mailto:<%= user.email %>">Email <%= user.username %></a></td>
  <% else %>
    <td>No contact info provided</td>
  <% end %>
  <!-- <td><%#= link_to "Destroy", book, method: :delete, data: { confirm: "Are you sure?" } %></td> -->
</tr>
