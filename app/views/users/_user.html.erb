<tr>
  <td><%= user.username %></td>
  <% if !user.email.blank? %>
    <td><a href="mailto:<%= user.email %>"><%= user.email %></a></td>
  <% else %>
    <td>No contact info provided</td>
  <% end %>
    <td><%= link_to "Show", user_path(user) %></td>
    <td><%= link_to "Edit", edit_user_path(user) %></td>
</tr>
