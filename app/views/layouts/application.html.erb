<!DOCTYPE html>
<html>
  <head>
    <title>ProjectManagerV001</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>


  <body>


  <!-- template for Navbar -->
    <div class="navbar navbar-inverse navbar-fixed-top" role="navigation">
      <div class="container">
        <div class="navbar-header">
          <a class="navbar-brand" href="#">Home</a>
          <a class="navbar-brand" href="#">Sign Up</a>
          <a class="navbar-brand" href="#">Log in</a>
          <a class="navbar-brand" href="<%= new_project_path %>">Create Project</a>
          <a class="navbar-brand" href="<%= projects_path %>">Show All Projects</a>
          <a class="navbar-brand" href="#">Create Task</a>
          <a class="navbar-brand" href="#">Log out</a>
        </div>
      </div>
    </div>
    <div class="center">
      <div class="info">
        <div class="container">
        <%= yield %>
        </div>
      </div>
    </div>

  </body>

  <!-- Footer template -->
  <footer class="center">
		<p>Helpful Research Resources:<br>
			<a href="http://wikipedia.org" >Wikipedia</a><br>
			<a href="http://google.com" >Google it!</a><br>
			<a href="mailto: support@systems.com">Email Support</a>
		</p>
	</footer>

</html>
