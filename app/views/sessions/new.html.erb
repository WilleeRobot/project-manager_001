  <h1>LOGIN</h1>
  <%= link_to('Log in with Facebook!', '/auth/facebook') %> 
  <%= form_tag(sessions_path) do %>
    <p><%= text_field_tag :username, "",:placeholder => "Enter username" %></p>
    <p><%= password_field_tag :password, "", :class => "new-todo", :placeholder => "Enter password" %></p>
    <%= submit_tag "Login"%>
  <% end %>
